[
    {
        "test_name": "opkg_install_dependencies",
        "commands": [
            "opkg update",
            "[[ -z $(opkg list-installed | grep -qw 'node') ]] && export RESULT='Installed'|| export RESULT1='Not found'",
            "[[ $RESULT=='Not found' ]] && [[ -z $(opkg install node | grep -q 'Configuring node') ]] && RESULT='Installed'",
            "wait_and_reset_serial_buffers 0",
            "echo $RESULT && unset RESULT"
        ],
        "expected_response": "Installed",
        "timeout": 60,
        "if_failed": "https://documentation.onioniot.com/software/node"
    },
    {
        "test_name": "check_node_version",
        "commands": [
            "node -v"
        ],
        "expected_response": "v18.19.1",
        "timeout": 15,
        "if_failed": ""
    }
]